{
  "name": "protectedDataSharing-dapp",
  "version": "0.0.1",
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "scripts": {
    "start": "tsx src/app.ts",
    "start-local": "tsx -r dotenv/config src/app.ts",
    "test": "jest",
    "test:silent": "jest --silent=true --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-format": "prettier --check '{src,tests,deployment/src}/**/*.ts'",
    "format": "prettier --write '{src,tests,deployment/src}/**/*.ts'",
    "deploy-app": "tsx ./src/deployScript.ts",
    "configure-app-ens": "tsx ./deployment/src/configureEnsNameScript.ts",
    "create-app-whitelist": "tsx ./deployment/src/createAppWhitelistScript.ts",
    "add-app-to-whitelist": "tsx ./deployment/src/addAppToWhitelistScript.ts",
    "transfer-app-ownership": "tsx ./deployment/src/transferOwnershipScript.ts",
    "refresh-abis": "node deployment/tools/refreshAbis.mjs"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.3.100",
    "@swc/jest": "^0.1.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "yup": "^1.3.3"
  }
}
