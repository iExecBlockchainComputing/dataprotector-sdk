{
  "name": "deployment-dapp",
  "version": "0.0.1",
  "description": "deploy dapp contract on iexec",
  "type": "module",
  "scripts": {
    "deploy-dapp": "tsx ./src/deployScript.ts",
    "publish-sell-order": "tsx ./src/publishSellOrderScript.ts",
    "configure-ens": "tsx ./src/configureEnsNameScript.ts",
    "revoke-sell-order": "tsx ./src/revokeSellOrderScript.ts",
    "create-dapp-whitelist": "tsx ./src/createAppWhitelistScript.ts",
    "transfer-ownership": "tsx ./src/transferOwnershipScript.ts",
    "check-format": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "refresh-abis": "node tools/refreshAbis.mjs"
  },
  "dependencies": {
    "ethers": "^6.9.2",
    "iexec": "^8.5.1",
    "tsx": "^4.6.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "prettier": "^3.1.1",
    "typescript": "^4.9.5"
  }
}
