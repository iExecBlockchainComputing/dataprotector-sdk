{
  "name": "DataProtector-v2",
  "version": "0.1.2",
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"(src|tests)/**/*.ts\"",
    "codegen": "./generate-manifest.sh && graph codegen",
    "build": "./generate-manifest.sh && graph build",
    "deploy": "./generate-manifest.sh && graph deploy --node https://api.thegraph.com/deploy/ DataProtector",
    "create-local": "graph create --node http://localhost:8020/ DataProtector-v2",
    "remove-local": "graph remove --node http://localhost:8020/ DataProtector-v2",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 DataProtector-v2",
    "all": "rm -rf generated && rm -rf build && npm run codegen && npm run build && npm run create-local && npm run deploy-local",
    "test": "graph test",
    "coverage": "graph test -- -c"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.46.1",
    "@graphprotocol/graph-ts": "0.29.1",
    "@iexec/poco": "^5.3.0",
    "matchstick-as": "^0.5.0"
  },
  "devDependencies": {
    "prettier": "^2.8.4"
  }
}
